/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    void deleteNode(ListNode* node) {
       ListNode* newNode= new ListNode;
        newNode->val=node->next->val;
        newNode->next=node->next->next;
        *node=*newNode;
    }
};
